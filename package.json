{
	"name": "livelorules",
	"displayName": "LiveloRules",
	"description": "Extens√£o desenvolvida para verificar regras Livelo",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other",
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:livelorules.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "livelorules.helloWorld",
				"title": "Livelo rules"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
